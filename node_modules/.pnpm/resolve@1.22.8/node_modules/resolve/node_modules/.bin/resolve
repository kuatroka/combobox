#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules/resolve/bin/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules/resolve/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules/resolve/bin/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules/resolve/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/resolve@1.22.8/node_modules:/Users/yo_macbook/Documents/dev/phoenix-analytics-test/combobox/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
